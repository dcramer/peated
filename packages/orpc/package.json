{
  "private": true,
  "name": "@peated/orpc",
  "type": "module",
  "exports": {
    "./client/*": {
      "types": "./dist/client/*.d.ts",
      "default": "./src/client/*.ts"
    },
    "./server/*": {
      "types": "./dist/server/*.d.ts",
      "default": "./src/server/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsdown --watch",
    "lint": "biome check",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@orpc/client": "^1.3.0",
    "@orpc/shared": "^1.3.0",
    "@orpc/server": "^1.3.0",
    "tsdown": "^0.12.6"
  },
  "dependencies": {
    "@peated/tsconfig": "workspace:*",
    "@sentry/core": "*"
  },
  "peerDependencies": {
    "@sentry/core": "*"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
