{
  "private": true,
  "name": "api",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node --require dotenv/config ./src/server.ts",
    "dev:load-mocks": "ts-node --require dotenv/config ./src/bin/load-mocks.ts",
    "db:deploy": "npx prisma migrate deploy",
    "db:migrate": "ts-node --require dotenv/config ./src/bin/migrate.ts",
    "db:generate": "npx drizzle-kit generate:pg",
    "generate-token": "ts-node --require dotenv/config ./src/bin/generate-token.ts",
    "user:create": "ts-node --require dotenv/config ./src/bin/create-user.ts",
    "user:admin": "ts-node --require dotenv/config ./src/bin/make-admin.ts",
    "user:generate-token": "ts-node --require dotenv/config ./src/bin/generate-token.ts",
    "postinstall": "npx prisma generate",
    "tsc": "tsc",
    "start": "ts-node --require dotenv/config ./src/server.ts",
    "test": "run-s test:vitest",
    "test:vitest": "dotenv -e .env.test -- vitest --single-thread"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/helmet": "^10.1.1",
    "@fastify/multipart": "^7.6.0",
    "@fastify/one-line-logger": "^1.1.1",
    "@google-cloud/storage": "^6.10.1",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.36.6",
    "@opentelemetry/sdk-node": "^0.38.0",
    "@paralleldrive/cuid2": "^2.2.0",
    "@peated/shared": "file:../../packages/shared",
    "@prisma/client": "^4.12.0",
    "@sentry/node": "^7.51.0",
    "@sentry/opentelemetry-node": "^7.51.0",
    "@sentry/profiling-node": "^0.3.0",
    "@sentry/utils": "^7.51.0",
    "@swc/core": "^1.3.57",
    "@swc/helpers": "^0.4.14",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.11",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.25.4",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "google-auth-library": "^8.8.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "mime-types": "^2.1.35",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "pg": "^8.10.0",
    "pino-pretty": "^10.0.0",
    "prisma": "^4.12.0",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8",
    "@types/pg": "^8.6.6",
    "drizzle-kit": "^0.17.6"
  }
}
