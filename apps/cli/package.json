    "@trpc/client": "11.0.0-rc.334",
    "@trpc/server": "11.0.0-rc.334",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "bcrypt": "^5.1.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.23.0",
    "drizzle-orm": "^0.32.0",
    "pg": "^8.12.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.0",
    "typescript": "~5.4.5"
