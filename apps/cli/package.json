{
  "private": true,
  "name": "@peated/cli",
  "scripts": {
    "build": "tsc",
    "cli": "tsx --require dotenv/config ./src/index.ts",
    "db": "tsx --require dotenv/config ./src/index.ts db",
    "bottles": "tsx --require dotenv/config ./src/index.ts bottles",
    "entities": "tsx --require dotenv/config ./src/index.ts entities",
