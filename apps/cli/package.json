    "commander": "^10.0.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.6",
    "esbuild": "^0.19.12",
    "esbuild-node-tsc": "^2.0.5",
    "tsx": "*",
