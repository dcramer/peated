{
  "private": true,
  "name": "@peated/web",
  "scripts": {
    "assets": "npx @svgr/cli --out-dir app/components/assets -- app/assets",
    "assets:watch": "npm-watch assets",
    "build": "npm run build:remix && npm run build:server && npm run build:assets && npm run build:sitemap && npm run sentry:upload-sourcemaps && find ./public/build -type f -name '*.map' -delete && find ./build -type f -name '*.map' -delete",
    "build:remix": "cross-env NODE_ENV=production remix build --sourcemap",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --sourcemap",
    "build:assets": "npm run assets",
    "build:sitemap": "remix-sitemap",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development nodemon --exec 'esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle && node --require dotenv/config ./build/server.js' --watch ./server.ts",
    "dev:assets": "npm run assets && npm run assets:watch",
    "sentry:upload-sourcemaps": "if test \"$NODE_ENV\" = \"production\"; then node ./node_modules/@sentry/remix/scripts/sentry-upload-sourcemaps.js --release $SENTRY_RELEASE; fi",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/raleway": "^5.0.19",
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.3",
    "@hookform/resolvers": "^3.4.0",
    "@peated/design": "workspace:*",
    "@peated/server": "workspace:*",
    "@peated/tsconfig": "workspace:*",
    "@react-oauth/google": "^0.11.1",
    "@remix-run/css-bundle": "^2.9.2",
    "@remix-run/dev": "^2.9.2",
    "@remix-run/eslint-config": "^2.9.2",
    "@remix-run/express": "^2.9.2",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@remix-run/server-runtime": "^2.9.2",
    "@rollup/plugin-replace": "^5.0.5",
    "@sentry-internal/feedback": "~8.2.1",
    "@sentry/remix": "^8.9.2",
    "@sentry/vite-plugin": "^2.18.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/react-query": "^5.37.1",
    "@trpc/client": "11.0.0-rc.334",
    "@trpc/react-query": "11.0.0-rc.334",
    "@trpc/server": "11.0.0-rc.334",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/leaflet": "^1.9.12",
    "@types/morgan": "^1.9.9",
    "@types/pica": "^9.0.4",
    "@types/react": "^18.3.2",
    "@types/react-avatar-editor": "^13.0.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.19",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "express": "^4.19.2",
    "framer-motion": "^10.18.0",
    "isbot": "^5.1.6",
    "isomorphic-dompurify": "^2.10.0",
    "leaflet": "^1.9.4",
    "marked": "^9.1.6",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "npm-watch": "^0.11.0",
    "pica": "^9.0.1",
    "postcss": "^8.4.38",
    "prom-client": "^15.1.2",
    "react": "^18.3.1",
    "react-avatar-editor": "^13.0.2",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.4",
    "react-leaflet": "^4.2.1",
    "react-qr-code": "^2.0.13",
    "react-router-dom": "^6.23.1",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.5.0",
    "remix-hook-form": "^3.2.3",
    "remix-sitemap": "^3.2.0",
    "tailwindcss": "^3.4.3",
    "tiny-invariant": "^1.3.3",
    "ts-debounce": "^4.0.0",
    "typescript": "~5.4.5",
    "use-dehydrated-state": "^0.1.0",
    "usehooks-ts": "^2.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@spotlightjs/spotlight": "2.0.0-alpha.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "watch": {
    "assets": {
      "patterns": [
        "assets"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
