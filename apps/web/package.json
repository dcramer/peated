{
  "private": true,
  "name": "@peated/web",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:remix && npm run build:server && npm run build:sitemap",
    "build:remix": "cross-env NODE_ENV=production remix vite:build && npm run sentry:upload-sourcemaps && find ./build -type f -name '*.map' -delete",
    "build:server": "esno ./support/build-server.ts",
    "build:sitemap": "remix-sitemap",
    "predev": "pnpm run build:remix",
    "dev": "cross-env NODE_ENV=development node ./server/dev-server.js",
    "sentry:upload-sourcemaps": "if test \"$NODE_ENV\" = \"production\"; then node ./node_modules/@sentry/remix/scripts/sentry-upload-sourcemaps.js --release $SENTRY_RELEASE; fi",
    "start": "cross-env NODE_ENV=production node ./index.js",
    "typecheck": "tsc --noEmit --incremental"
  },
  "exports": {
    "./*": [
      "./app/*",
      "./app/*.ts",
      "./app/*.tsx",
      "./app/*/index.ts",
      "./app/*/index.tsx"
    ]
  },
  "dependencies": {
    "@fontsource/raleway": "^5.0.17",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.4",
    "@peated/design": "workspace:*",
    "@peated/server": "workspace:*",
    "@peated/tsconfig": "workspace:*",
    "@react-oauth/google": "^0.11.1",
    "@remix-run/express": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@remix-run/server-runtime": "^2.8.1",
    "@rollup/plugin-replace": "^5.0.5",
    "@sentry-internal/feedback": "^7.106.1",
    "@sentry/react": "^7.106.1",
    "@sentry/remix": "^7.106.1",
    "@sentry/vite-plugin": "^2.14.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "address": "^2.0.2",
    "close-with-grace": "^1.3.0",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "express": "^4.18.3",
    "fast-glob": "^3.3.2",
    "framer-motion": "^10.18.0",
    "get-port": "^7.0.0",
    "isbot": "latest",
    "isomorphic-dompurify": "^2.4.0",
    "leaflet": "^1.9.4",
    "marked": "^9.1.6",
    "morgan": "^1.10.0",
    "pica": "^9.0.1",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-avatar-editor": "^13.0.2",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.3",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "remix-sitemap": "^3.2.0",
    "tiny-invariant": "^1.3.3",
    "ts-debounce": "^4.0.0",
    "use-dehydrated-state": "^0.1.0",
    "usehooks-ts": "^3.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@spotlightjs/spotlight": "^1.2.13",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/leaflet": "^1.9.8",
    "@types/morgan": "^1.9.9",
    "@types/pica": "^9.0.4",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^18.2.65",
    "@types/react-avatar-editor": "^13.0.2",
    "@types/react-dom": "^18.2.21",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.18",
    "esbuild": "^0.20.1",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vite": "^5.1.6",
    "vite-plugin-cjs-interop": "^2.1.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "watch": {
    "assets": {
      "patterns": [
        "assets"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
