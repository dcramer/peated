{
  "private": true,
  "name": "@peated/server",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently 'npm:dev:api' 'npm:dev:worker'",
    "dev:api": "tsx watch --require dotenv/config ./src/run-server.ts",
    "dev:worker": "tsx watch --require dotenv/config ./src/run-worker.ts",
    "start": "concurrently 'npm:start:api' 'npm:start:worker'",
    "start:api": "tsx --require dotenv/config ./src/run-server.ts",
    "start:worker": "tsx --require dotenv/config ./src/run-worker.ts",
    "typecheck": "tsc --noEmit",
    "test": "dotenv -e .env.test -- vitest",
    "test:watch": "dotenv -e .env.test -- vitest watch",
    "test:ci": "dotenv -e .env.test -- vitest --coverage --reporter=junit --reporter=default --outputFile=junit.xml"
  },
